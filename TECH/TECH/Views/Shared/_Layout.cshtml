         <script type="text/javascript">
        $(document).ready(function() {

            $(".btn-search-stl").click(function(){
                var textSearch = $(".txt-search").val();
                 if (textSearch !=""){
                       window.location.href = '/san-pham-tim-kiem/timkiem=' + textSearch;
                 }              
            })

            $(".txt-search").keypress(function (e) {
              if (e.which == 13) {
                var textSearch = $(".txt-search").val();
                if (textSearch !=""){
                      window.location.href = '/san-pham-tim-kiem/timkiem=' + textSearch;
                return false;   
                }
              
              }
            });

            var timkiem = urlParam();
           
            if(timkiem != null && timkiem != ""){
                debugger;
                var text = timkiem.split('=')[1];
                $(".txt-search").val(text);
            }
        })
        function urlParam(){
            var results = new RegExp('timkiem=[^&#]*').exec(window.location.href);
            if (results==null) {
               return null;
            }
            return decodeURI(results[0]) || "";
        }


    </script>
